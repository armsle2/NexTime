<div class="container landing">
    
    <div class="row">
        <div class="col-sm-12">
            <div class="row">
                <h1>NexTime</h1>
                <h3>The greatest app ever!</h3>
            </div>
            <div class = "userContainer">
                <button type="submit" class="btn btn-success"><a href="/to-do">Go to To-Do List</a></button>  
                <ul>
                    {{#each tasks}}
                    {{math @index '+' 1}}
                    <li>{{this.task}}</li>
                    {{/each}}
                </ul>
            </div>
        </div>
    </div>