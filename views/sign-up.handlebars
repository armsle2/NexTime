<div class="boxindex">
    <form id="sign-in-form">
        <h2 class="form-title">Create an Account</h2>
        <div class="username-taken"></div>
        <div class="form-group">
            <input type="text" class="form-control animated" id="first-name-sign-up" placeholder="First Name">
            <input type="text" class="form-control animated" id="last-name-sign-up" placeholder="Last Name">
            <input type="text" class="form-control animated" id="username-sign-up" placeholder="Username">
            <input type="password" class="form-control animated" id="password-sign-up" placeholder="Password">
            <input type="submit" class="btn btn-primary btn-lg btn-block submit" id= "submit-sign-up"></input>
        </div>
        <div class="back"><a href="/" class="back">Back</a>
    </form>
</div>
